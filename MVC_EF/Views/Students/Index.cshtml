@model IEnumerable<MVC_EF.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Students</h2>

@foreach (var item in Model) {
    <a href="#" class="btn btn-primary"Go somewhere @Html.ActionLink("Details", "Details", new { id = item.StudentID })</a>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FavoriteColor)
        </td>
    </tr>
}
